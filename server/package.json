{
  "name": "nightchef-serv",
  "version": "0.0.1",
  "description": "web-server for NightChef.",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc --extendedDiagnostics --pretty && copyfiles -u 2 src/graphql/schema.graphql dist/src/graphql",
    "start": "node dist/src/index.js -s",
    "mock": "node dist/src/index.js -m",
    "dev": "tsc-watch --project . --outDir ./dist --onSuccess \"npm run start\"",
    "clean": "del-cli -f dist",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 2"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/smallwood-d/NightChef.git"
  },
  "author": "---",
  "license": "SEE LICENSE IN license.md",
  "bugs": {
    "url": "https://github.com/smallwood-d/NightChef/issues"
  },
  "homepage": "https://github.com/smallwood-d/NightChef#readme",
  "dependencies": {
    "apollo-server-express": "^3.3.0",
    "argparse": "^2.0.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "moment": "^2.27.0",
    "mongodb": "^4.1.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@microsoft/sp-tslint-rules": "^1.11.0",
    "@types/argparse": "^2.0.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.1",
    "@types/node": "^14.11.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "copyfiles": "^2.4.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.32.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.0.3"
  }
}
